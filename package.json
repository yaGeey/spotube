{
   "name": "spotube",
   "private": true,
   "version": "0.0.1",
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "tsc && vite build && electron-builder",
      "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
      "lint.err": "eslint . --ext ts,tsx --quiet",
      "preview": "vite preview",
      "rebuild": "electron-rebuild -f -w better-sqlite3",
      "postinstall": "electron-builder install-app-deps"
   },
   "build": {
      "appId": "com.spotube.app",
      "productName": "Spotube",
      "publish": [
         {
            "provider": "github",
            "owner": "yaGeey",
            "repo": "spotube"
         }
      ],
      "directories": {
         "buildResources": "assets"
      },
      "mac": {
         "target": [
            "dmg",
            "zip"
         ]
      },
      "win": {
         "target": [
            "nsis",
            "zip"
         ]
      },
      "linux": {
         "target": [
            "AppImage",
            "deb"
         ]
      }
   },
   "dependencies": {
      "@ai-sdk/google": "^3.0.7",
      "@fontsource-variable/roboto-flex": "^5.2.8",
      "@fortawesome/fontawesome-svg-core": "^7.1.0",
      "@fortawesome/free-regular-svg-icons": "^7.1.0",
      "@fortawesome/free-solid-svg-icons": "^7.1.0",
      "@fortawesome/react-fontawesome": "^3.1.1",
      "@google/genai": "^1.34.0",
      "@googleapis/customsearch": "^7.0.1",
      "@prisma/adapter-better-sqlite3": "^7.1.0",
      "@prisma/client": "^7.1.0",
      "@tailwindcss/typography": "^0.5.19",
      "@tailwindcss/vite": "^4.1.17",
      "@tanstack/react-query": "^4.42.0",
      "@tanstack/react-query-devtools": "^4.42.0",
      "@tanstack/react-table": "^8.21.3",
      "@tanstack/react-virtual": "^3.13.18",
      "@tavily/core": "^0.6.4",
      "@trpc/client": "^10.45.4",
      "@trpc/react-query": "^10.45.4",
      "@trpc/server": "^10.45.4",
      "@types/cli-progress": "^3.11.6",
      "@types/discord-rpc": "^4.0.10",
      "@types/react-fontawesome": "^1.6.8",
      "@types/spotify-api": "^0.0.25",
      "ai": "^6.0.33",
      "axios": "^1.13.2",
      "better-sqlite3": "^12.4.1",
      "chalk": "^5.6.2",
      "cheerio": "^1.1.2",
      "cli-progress": "^3.12.0",
      "date-fns": "^4.1.0",
      "discord-rpc": "^4.0.1",
      "dotenv": "^17.2.3",
      "duck-duck-scrape": "^2.2.7",
      "electron-store": "^11.0.2",
      "electron-trpc": "^0.7.1",
      "genius-lyrics": "^4.4.7",
      "googleapis": "^166.0.0",
      "mutative": "^1.3.0",
      "p-limit": "^7.2.0",
      "p-retry": "^7.1.1",
      "react": "^19.2.0",
      "react-dom": "^19.2.0",
      "react-markdown": "^10.1.0",
      "react-router-dom": "^7.9.6",
      "react-toastify": "^11.0.5",
      "react-youtube": "^10.1.0",
      "superjson": "^2.2.6",
      "tailwind-merge": "^3.4.0",
      "tailwind-scrollbar": "^4.0.2",
      "tailwindcss": "^4.1.17",
      "ts-to-zod": "^5.1.0",
      "uuid": "^13.0.0",
      "youtubei": "^1.8.3",
      "youtubei.js": "^16.0.1",
      "zod": "^4.3.5",
      "zustand": "^5.0.8"
   },
   "devDependencies": {
      "@tanstack/eslint-plugin-query": "^5.91.2",
      "@types/better-sqlite3": "^7.6.13",
      "@types/node": "^25.0.2",
      "@types/react": "^19.2.6",
      "@types/react-dom": "^19.2.3",
      "@types/uuid": "^10.0.0",
      "@typescript-eslint/eslint-plugin": "^7.1.1",
      "@typescript-eslint/parser": "^7.1.1",
      "@vitejs/plugin-react": "^4.2.1",
      "babel-plugin-react-compiler": "^1.0.0",
      "electron": "^30.0.1",
      "electron-builder": "^24.13.3",
      "electron-rebuild": "^3.2.9",
      "eslint": "^8.57.0",
      "eslint-plugin-react": "^7.37.5",
      "eslint-plugin-react-hooks": "^7.0.1",
      "eslint-plugin-react-refresh": "^0.4.24",
      "prisma": "^7.1.0",
      "prisma-json-types-generator": "^4.0.1",
      "typescript": "^5.2.2",
      "vite": "^5.1.6",
      "vite-plugin-electron": "^0.28.6",
      "vite-plugin-electron-renderer": "^0.14.5"
   },
   "main": "dist-electron/main.js"
}
